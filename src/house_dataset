from vowpalwabbit import pyvw
model = pyvw.vw(quiet=False)

with open("C:\\Users\\konra\\PycharmProjects\\Sentiment_analysis\\src\\house_dataset") as fp:
    Lines = fp.readlines()
    for line in Lines:
        model.learn(line)
